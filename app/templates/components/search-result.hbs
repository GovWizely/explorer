<h3><a href="#" {{action "toggleBody"}}>{{searchHeader}}</a></h3>
{{#if isShowingBody}}
  {{#with document}}
    <table class="pure-table pure-table-bordered">
      {{#if source}}
        <tr><td>Source</td><td>{{source}}</td></tr>
      {{/if}}
      {{#if start_date}}
        <tr><td>Start Date</td><td>{{start_date}}</td></tr>
      {{/if}}
      {{#if end_date}}
        <tr><td>End Date</td><td>{{end_date}}</td></tr>
      {{/if}}

      {{#if entity_number}}
        <tr><td>Entity Number</td><td>{{entity_number}}</td></tr>
      {{/if}}
      {{#if type}}
        <tr><td>Type</td><td>{{type}}</td></tr>
      {{/if}}
      {{#if title}}
        <tr><td>Title</td><td>{{title}}</td></tr>
      {{/if}}
      {{#if federal_register_notice}}
        <tr><td>Federal Register Notice</td><td>{{federal_register_notice}}</td></tr>
      {{/if}}
      {{#if standard_order}}
        <tr><td>Standard Order</td><td>{{standard_order}}</td></tr>
      {{/if}}
      {{#if license_requirement}}
        <tr><td>License Requirement</td><td>{{license_requirement}}</td></tr>
      {{/if}}
      {{#if license_policy}}
        <tr><td>License Policy</td><td>{{license_policy}}</td></tr>
      {{/if}}
      {{#if call_sign}}
        <tr><td>Call Sign</td><td>{{call_sign}}</td></tr>
      {{/if}}
      {{#if vessel_type}}
        <tr><td>Vessel Type</td><td>{{vessel_type}}</td></tr>
      {{/if}}
      {{#if gross_tonnage}}
        <tr><td>Gross Tonnage</td><td>{{gross_tonnage}}</td></tr>
      {{/if}}
      {{#if gross_registered_tonnage}}
        <tr><td>Gross Registered Tonnage</td><td>{{gross_registered_tonnage}}</td></tr>
      {{/if}}
      {{#if vessel_flag}}
        <tr><td>Vessel Flag</td><td>{{vessel_flag}}</td></tr>
      {{/if}}
      {{#if vessel_owner}}
        <tr><td>Vessel Owner</td><td>{{vessel_owner}}</td></tr>
      {{/if}}
      {{#if remarks}}
        <tr><td>Remarks</td><td>{{remarks}}</td></tr>
      {{/if}}
      {{#if source_list_url}}
        <tr><td>Source List URL</td><td><a href="{{unbound source_list_url}}" target="_blank">{{source_list_url}}</a></td></tr>
      {{/if}}
      {{#if source_information_url}}
        <tr><td>Source Information URL</td><td><a href="{{unbound source_information_url}}" target="_blank">{{source_information_url}}</a></td></tr>
      {{/if}}

      {{search-result-row-list label="Programs" items=programs}}
      {{search-result-row-list label="Alternative Names" items=alt_names}}
      {{search-result-row-list label="Nationalities" items=nationalities}}
      {{search-result-row-list label="Citizenships" items=citizenships}}
      {{search-result-row-list label="Dates of Birth" items=dates_of_birth}}
      {{search-result-row-list label="Places of Birth" items=places_of_birth}}

      {{#if addresses}}
        <tr><td>Addresses</td><td>
          <ol>
          {{#each a in addresses}}
            <li><ul>
              {{#if a.address}}
                <li>{{a.address}}</li>
              {{/if}}
              {{#if a.city}}
                <li>{{a.city}}</li>
              {{/if}}
              {{#if a.state}}
                <li>{{a.state}}</li>
              {{/if}}
              {{#if a.postal_code}}
                <li>{{a.postal_code}}</li>
              {{/if}}
              {{#if a.country}}
                <li>{{a.country}}</li>
              {{/if}}
            </ul></li>
          {{/each}}
          </ol>
        </td></tr>
      {{/if}}

      {{#if identifications}}
        <tr><td>IDs</td><td>
          <ol>
          {{#each id in identifications}}
            <li><ul>
              {{#if id.type}}
                <li>{{id.type}}</li>
              {{/if}}
              {{#if id.number}}
                <li>{{id.number}}</li>
              {{/if}}
              {{#if id.country}}
                <li>{{id.country}}</li>
              {{/if}}
              {{#if id.issue_date}}
                <li>{{id.issue_date}}</li>
              {{/if}}
              {{#if id.expiration_date}}
                <li>{{id.expiration_date}}</li>
              {{/if}}
            </ul></li>
          {{/each}}
          </ol>
        </td></tr>
      {{/if}}

      {{#if event_name}}
        <tr><td>Event Name</td><td>{{event_name}}</td></tr>
      {{/if}}
      {{#if event_type}}
        <tr><td>Event Type</td><td>{{event_type}}</td></tr>
      {{/if}}
      {{#if start_time}}
        <tr><td>Start Time</td><td>{{start_time}}</td></tr>
      {{/if}}
      {{#if end_time}}
        <tr><td>End Time</td><td>{{end_time}}</td></tr>
      {{/if}}
      {{#if time_zone}}
        <tr><td>Time Zone</td><td>{{time_zone}}</td></tr>
      {{/if}}
      {{#if cost}}
        <tr><td>Cost</td><td>{{cost}}</td></tr>
      {{/if}}
      {{#if cost_currency}}
        <tr><td>Cost Currency</td><td>{{cost_currency}}</td></tr>
      {{/if}}
      {{#if registration_link}}
        <tr><td>Registration Link</td><td><a href="{{unbound registration_link}}" target="_blank">{{registration_link}}</a></td></tr>
      {{/if}}
      {{#if registration_title}}
        <tr><td>Registration Title</td><td>{{registration_title}}</td></tr>
      {{/if}}
      {{#if description}}
        <tr><td>Description</td><td>{{description}}</td></tr>
      {{/if}}
      {{#if url}}
        <tr><td>URL</td><td><a href="{{unbound url}}" target="_blank">{{url}}</a></td></tr>
      {{/if}}
      {{#if venue}}
        <tr><td>Venue</td><td>{{venue}}</td></tr>
      {{/if}}
      {{#if city}}
        <tr><td>City</td><td>{{city}}</td></tr>
      {{/if}}
      {{#if state}}
        <tr><td>State</td><td>{{state}}</td></tr>
      {{/if}}
      {{#if country}}
        <tr><td>Country</td><td>{{country}}</td></tr>
      {{/if}}

      {{search-result-row-list label="Industries" items=industries}}

      {{#if contacts}}
        <tr><td>Contacts</td><td>
          <ol>
          {{#each contact in contacts}}
            <li><ul>
              {{#if contact.first_name}}
                <li>{{contact.first_name}}</li>
              {{/if}}
              {{#if contact.last_name}}
                <li>{{contact.last_name}}</li>
              {{/if}}
              {{#if contact.post}}
                <li>{{contact.post}}</li>
              {{/if}}
              {{#if contact.person_title}}
                <li>{{contact.person_title}}</li>
              {{/if}}
              {{#if contact.phone}}
                <li>{{contact.phone}}</li>
              {{/if}}
              {{#if contact.email}}
                <li>{{contact.email}}</li>
              {{/if}}
            </ul></li>
          {{/each}}
          </ol>
        </td></tr>
      {{/if}}

      {{#if venues}}
        <tr><td>Venues</td><td>
          <ol>
          {{#each venue in venues}}
            <li><ul>
              {{#if venue.venue}}
                <li>{{venue.venue}}</li>
              {{/if}}
              {{#if venue.city}}
                <li>{{venue.city}}</li>
              {{/if}}
              {{#if venue.state}}
                <li>{{venue.state}}</li>
              {{/if}}
              {{#if venue.country}}
                <li>{{venue.country}}</li>
              {{/if}}
            </ul></li>
          {{/each}}
          </ol>
        </td></tr>
      {{/if}}

      {{#if source_id}}
          <tr><td>source_id</td><td>{{source_id}}</td></tr>
      {{/if}}
      {{#if tariff_line}}
          <tr><td>tariff_line</td><td>{{tariff_line}}</td></tr>
      {{/if}}
      {{#if description}}
          <tr><td>description</td><td>{{description}}</td></tr>
      {{/if}}
      {{#if hs_6}}
          <tr><td>hs_6</td><td>{{hs_6}}</td></tr>
      {{/if}}
      {{#if sector_code}}
          <tr><td>sector_code</td><td>{{sector_code}}</td></tr>
      {{/if}}
      {{#if base_rate}}
          <tr><td>base_rate</td><td>{{base_rate}}</td></tr>
      {{/if}}
      {{#if base_rate_alt}}
          <tr><td>base_rate_alt</td><td>{{base_rate_alt}}</td></tr>
      {{/if}}
      {{#if final_year}}
          <tr><td>final_year</td><td>{{final_year}}</td></tr>
      {{/if}}
      {{#if tariff_lines_per_6_digit}}
          <tr><td>tariff_lines_per_6_digit</td><td>{{tariff_lines_per_6_digit}}</td></tr>
      {{/if}}
      {{#if tariff_rate_quota}}
          <tr><td>tariff_rate_quota</td><td>{{tariff_rate_quota}}</td></tr>
      {{/if}}
      {{#if tariff_rate_quota_note}}
          <tr><td>tariff_rate_quota_note</td><td>{{tariff_rate_quota_note}}</td></tr>
      {{/if}}
      {{#if tariff_eliminated}}
          <tr><td>tariff_eliminated</td><td>{{tariff_eliminated}}</td></tr>
      {{/if}}
      {{#if product_type_id}}
          <tr><td>product_type_id</td><td>{{product_type_id}}</td></tr>
      {{/if}}
      {{#if pending_data}}
          <tr><td>pending_data</td><td>{{pending_data}}</td></tr>
      {{/if}}
      {{#if ag_id}}
          <tr><td>ag_id</td><td>{{ag_id}}</td></tr>
      {{/if}}
      {{#if partner_name}}
          <tr><td>partner_name</td><td>{{partner_name}}</td></tr>
      {{/if}}
      {{#if reporter_name}}
          <tr><td>reporter_name</td><td>{{reporter_name}}</td></tr>
      {{/if}}
      {{#if staging_basket}}
          <tr><td>staging_basket</td><td>{{staging_basket}}</td></tr>
      {{/if}}
      {{#if staging_basket_id}}
          <tr><td>staging_basket_id</td><td>{{staging_basket_id}}</td></tr>
      {{/if}}
      {{#if num_mar_columns}}
          <tr><td>num_mar_columns</td><td>{{num_mar_columns}}</td></tr>
      {{/if}}
      {{#if partner_start_year}}
          <tr><td>partner_start_year</td><td>{{partner_start_year}}</td></tr>
      {{/if}}
      {{#if reporter_start_year}}
          <tr><td>reporter_start_year</td><td>{{reporter_start_year}}</td></tr>
      {{/if}}
      {{#if partner_agreement_name}}
          <tr><td>partner_agreement_name</td><td>{{partner_agreement_name}}</td></tr>
      {{/if}}
      {{#if reporter_agreement_name}}
          <tr><td>reporter_agreement_name</td><td>{{reporter_agreement_name}}</td></tr>
      {{/if}}
      {{#if partner_agreement_approved}}
          <tr><td>partner_agreement_approved</td><td>{{partner_agreement_approved}}</td></tr>
      {{/if}}
      {{#if reporter_agreement_approved}}
          <tr><td>reporter_agreement_approved</td><td>{{reporter_agreement_approved}}</td></tr>
      {{/if}}
      {{#if rule_text}}
          <tr><td>rule_text</td><td>{{rule_text}}</td></tr>
      {{/if}}
      {{#if link_text}}
          <tr><td>link_text</td><td>{{link_text}}</td></tr>
      {{/if}}
      {{#if link_url}}
          <tr><td>link_url</td><td>{{link_url}}</td></tr>
      {{/if}}
      {{#if quota_name}}
          <tr><td>quota_name</td><td>{{quota_name}}</td></tr>
      {{/if}}
      {{#if industry}}
          <tr><td>industry</td><td>{{industry}}</td></tr>
      {{/if}}
      {{#if annual_rates}}
          <tr><td>annual_rates</td><td>{{annual_rates}}</td></tr>
      {{/if}}
      {{#if alt_annual_rates}}
          <tr><td>alt_annual_rates</td><td>{{alt_annual_rates}}</td></tr>
      {{/if}}
      {{#if countries}}
          <tr><td>countries</td><td>{{countries}}</td></tr>
      {{/if}}
      {{#if source}}
          <tr><td>source</td><td>{{source}}</td></tr>
      {{/if}}

    </table>
  {{/with}}
{{/if}}
